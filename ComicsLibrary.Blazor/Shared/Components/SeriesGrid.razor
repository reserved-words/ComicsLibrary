@inherits Components.SeriesGridBase

<MudGrid Class="@(Visible ? "" : "d-none")">

    @foreach (var item in Items)
    {
        <MudItem xs="12" sm="4" md="3" lg="2" Class="@(item.Visible ? "" : "d-none")">
            <SeriesCard Series="@item" Actions="@Actions" OnAction="OnAction" />
        </MudItem>
    }
</MudGrid>